<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}System Newsletter{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('assets/css/bootstrap.min.css') }}" rel="stylesheet">
        {% endblock %}
    </head>
    <body>
        {% block body %}{#overwritten block in children#}{% endblock %}

        {% block javascripts %}
            <script src="{{ asset('assets/js/bootstrap.bundle.min.js') }}"></script>
            <script src="{{ asset('assets/js/jquery-3.5.1.min.js') }}"></script>
        {% endblock %}
    </body>
</html>
